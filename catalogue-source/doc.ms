226r ,
227v ;
259v ,
282r ,
283r ,
284v ,
286r ,
287r ,
289r ,
290r ,
291r) and D (ff. 116r ,
116v ,
119r ,
119v ,
121v ,
125v ;
212r ,
217r ,
217v ,
218r ,
219v ,
220v ,
222r ;
